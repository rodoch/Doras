<?xml version="1.0" encoding="utf-8"?>
<security>
  <services>
    <service prefix="asset/" name="AzureImageService" type="Doras.AzureImageService, Doras">
      <settings>
        <setting key="StorageAccount" value="DefaultEndpointsProtocol=https;AccountName=[YOUR_ACCOUNT_NAME];AccountKey=[YOUR_ACCOUNT_KEY];EndpointSuffix=core.windows.net" />
        <setting key="Container" value="[CONTAINER_NAME]"/>
        <setting key="AccessType" value="Off"/>
      </settings>
    </service>
  </services>
</security>